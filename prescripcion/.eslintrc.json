{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "indent": ["error", 4],
    "linebreak-style": ["error", "unix"],
    // "quotes": [
    //     "error",
    //     "double"
    // ],
    // "semi": [
    //     "error",
    //     "always"
    // ],
    "no-console": ["warn"],
    "no-unused-vars": ["warn"]
  }
}
